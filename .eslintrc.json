{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console": "off",
        "comma-dangle": "off",
        "Parsing error": "off",
        "consistent-return": "off",
        "curly": "off",
        "consistent - return": "off",
        "arrow-body-style": "off",
    }
}